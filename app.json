{
  "expo": {
    "name": "Dig Watch",
    "slug": "Dig-Watch-mobile-app",
    "version": "1.0.0",
    "description": "Diplodokus",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./digwatch-icon.png",
    "notification": {
      "icon": "./dw-push-logo.png"
    },
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Diplo.Diplo",
      "icon": "./digwatch-icon.png",
      "infoPlist": {
        "NSNotification": "Dig Watch would like to send you notification"
      }
    },
    "android": {
      "package": "com.Diplo.Diplo",
      "versionCode": 1,
      "icon": "./digwatch-icon.png",
      "permissions": ["NOTIFICATIONS"]
    }
  }
}