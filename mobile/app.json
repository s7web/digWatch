{
  "expo": {
    "name": "DW Just-in-Time Reporting",
    "slug": "dig-watch",
    "version": "1.0.6",
    "description": "DW Just-in-Time Reporting",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./digwatch-icon.png",
    "notification": {
      "icon": "./dw-push-logo.png"
    },
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diplo.digwatch",
      "icon": "./digwatch-icon.png",
      "infoPlist": {
        "NSNotification": "Dig Watch would like to send you notification"
      }
    },
    "android": {
      "package": "com.Dig.Watch",
      "versionCode": 6,
      "icon": "./digwatch-icon.png",
      "permissions": [
        "NOTIFICATIONS"
      ]
    }
  }
}
